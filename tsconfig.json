{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "typeRoots": [
      "./types"
    ],
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/layouts/*": [
        "layouts/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/themes": [
        "themes/index.ts"
      ],
      "@/store": [
        "store/index.tsx"
      ],
      "@/store/*": [
        "store/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/configs": [
        "configs.ts"
      ],
      "@/types/*": [
        "types/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
